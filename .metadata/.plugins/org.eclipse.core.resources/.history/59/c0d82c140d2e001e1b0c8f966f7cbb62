package maccess;

import java.util.Scanner;

public class Fibonacci {

	public static void main(String[] args) {
	  
		int a=0,b=1,c,term;
		System.out.println("Enter term");
		Scanner r = new Scanner(System.in);
		term= r.nextInt();
		
		for(int i=0; i<=term; i++) {
			System.out.println(a);
		        c=a+b;
		        b=a;
		        b=c;
		}

	}

}
